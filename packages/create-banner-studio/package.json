{
  "name": "create-banner-studio",
  "version": "0.1.0",
  "description": "Develop HTML banners with no build configuration",
  "repository": {
    "type": "git",
    "url": "https://github.com/aptas/create-banner-studio.git",
    "directory": "packages/create-banner-studio"
  },
  "author": "Magnus Bergman <magnus@apt.no>",
  "contributors": [
    "Anders Stalheim Ã˜fsdahl <anders@apt.no>"
  ],
  "license": "MIT",
  "bin": {
    "create-banner-studio": "lib/index.js"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "banner",
    "banner-studio",
    "cli-app",
    "cli"
  ],
  "scripts": {
    "lint": "eslint lib test",
    "format": "prettier --write '{lib,test}/**/*.js'",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "yarn test -- --reporter min --watch",
    "test:cover": "nyc yarn test --reporter=html && opn coverage/index.html",
    "coveralls": "nyc yarn test:cover && nyc report --reporter=text-lcov | coveralls"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "expect": "^24.8.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "opn-cli": "^5.0.0",
    "prettier": "^1.17.1"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^8.0.1",
    "hyperquest": "^2.1.3",
    "semver": "^6.1.0",
    "tar-pack": "^3.4.1",
    "tmp": "^0.1.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "peerDependencies": {}
}
